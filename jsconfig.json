{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "target": "ES6",
        "baseUrl": "./",
        "paths": {
            "@gxui": [
                "./src/index.js"
            ],
            "@gxui/*": [
                "./src/*"
            ],
            "@docs": [
                "./docs/index.js"
            ],
            "@docs/*": [
                "./docs/*"
            ],
        }
    },
    "exclude": [
        "node_modules"
    ],
}