{
    "presets": [
        "@babel/env",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        [
            "prismjs",
            {
                "languages": [
                    "html",
                    "css",
                    "javascript",
                    "sql"
                ],
                "plugins": [
                    "line-numbers"
                ],
                "theme": "tomorrow",
                "css": true
            }
        ]
    ]
}